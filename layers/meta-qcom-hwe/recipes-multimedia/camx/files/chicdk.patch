+static const DOUBLE IMX219MinRealGain            = 1.0;                                 ///< Minimum Analogue real gain (1X)
+static const DOUBLE IMX219MaxRealGain            = 10.66;                               ///< Maximum Analogue real gain (11X)
+static const DOUBLE IMX219MinDigitalGain         = 1.0;                                //   Minimum Digital real gain (1X)
+static const DOUBLE IMX219MaxDigitalGain         = 16.0;                                // Maximum Digital real gain (16X)
+static const DOUBLE IMX219TolalGain              = 27.0;                                //// Analogue + Digital sensor real gain (10.66x+16x)
+static const UINT   IMX219MinDigitalRegisterGain = 256;                                   ///< Minimum digital register gain
+static const UINT   IMX219MaxDigitalRegisterGain = 256;                                   ///< Maximum digital register gain
+static const UINT   IMX219DigitalGainDecimator   = 256;                                   /// < Digital gain decimator factor
+static const DOUBLE IMX219MinDigitalRealGain     = (IMX219MinDigitalRegisterGain / IMX219DigitalGainDecimator);
+                                                                                          ///< Minimum digital real gain (1X)
+static const DOUBLE IMX219MaxDigitalRealGain     = (IMX219MaxDigitalRegisterGain/ IMX219DigitalGainDecimator);
+                      
+////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
+// RegisterToRealGain
+////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
+static DOUBLE RegisterToRealGain(
+    UINT registerGain)
+{
+    if(IMX219MaxRegisterGain < registerGain)
+    {
+        registerGain = IMX219MaxRegisterGain;
+    }
+
+    return  (256.0 / (256.0 - registerGain));
+}
+
+////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
+// RealToRegisterGain
+////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
+static UINT RealToRegisterGain(
+    DOUBLE realGain)
+{
+    if (IMX219MinRealGain > realGain)
+    {
+        realGain = IMX219MinRealGain;
+    }
+    else if (IMX219TolalGain < realGain)
+    {
+        realGain = IMX219TolalGain;
+    }
+
+    return static_cast<UINT>(256.0 -(256.0/realGain));
+}
+
+////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
+// CalculateDigitalGain
+////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
+static UINT CalculateDigitalGain(
+    FLOAT totalRealGain,
+    FLOAT sensorRealGain)
+{
+    float digitalRealGain = IMX219MinDigitalRegisterGain;
+
+    if (IMX219MaxRealGain < totalRealGain)
+    {
+        digitalRealGain = totalRealGain / sensorRealGain;
+    }
+    else
+    {
+        digitalRealGain = IMX219MinDigitalRealGain;
+    }
+
+    if (IMX219MaxDigitalRealGain < digitalRealGain)
+    {
+        digitalRealGain = IMX219MaxDigitalRealGain;
+    }
+
+    return static_cast<UINT>(digitalRealGain * IMX219DigitalGainDecimator);
+}
